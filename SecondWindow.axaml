<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="250"
        x:Class="bios_vil3.SecondWindow"
        Title="Add Employee">
    <StackPanel Margin="10">
        <TextBox x:Name="FirstNameTextBox" Watermark="Imie" Margin="0,5"/>
        <TextBox x:Name="LastNameTextBox" Watermark="Nazwisko" Margin="0,5"/>
        <TextBox x:Name="AgeTextBox" Watermark="Wiek" Margin="0,5"/>
        <TextBox x:Name="PositionTextBox" Watermark="Zatrudnienie" Margin="0,5"/>
        <Button Content="Add Employee" Click="AddEmployee_Click" HorizontalAlignment="Center" Margin="0,10"/>
    </StackPanel>
</Window>